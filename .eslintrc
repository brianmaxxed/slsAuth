{
  "extends": "airbnb",
  "env": {
    "node": true,
    "jest": true,
  },
  "plugins": [
    "jest"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "consistent-return": 0,
    "max-len": [ 0, { code: 80, ignoreUrls: true, ignoreStrings: true, ignoreTemplateLiterals: true }],
    "jest/no-disabled-tests": 0,
    "jest/no-focused-tests": 0,
    "jest/no-identical-title": 1,
    "jest/prefer-to-have-length": 2,
    "jest/valid-expect": 1
  }
}
