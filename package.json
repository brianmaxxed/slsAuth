{
  "name": "serverlessAuth",
  "version": "0.2.0",
  "main": "server.js",
  "repository": "git@github.com/brianmaxxed/serverlessAuth.git",
  "author": "Brian Lewis <brian.maxx@gmail.com>",
  "description": "an example of serverless express nodejs",
  "license": "UNLICENSED",
  "engines": {
    "node": "8.10"
  },
  "watch": {
    "test:watch": "{src}/*.js"
  },
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  },
  "scripts": {
    "start": "yarn build && yarn server",
    "build": "webpack",
    "deploy:sls:dev": "NODE_ENV=development yarn build && sls deploy --stage dev --verbose",
    "deploy:sls:stage": "NODE_ENV=stage yarn build && sls deploy --stage stage --verbose",
    "deploy:sls:prod": "NODE_ENV=production yarn build && sls deploy --stage prod --verbose",
    "watch": "webpack --watch --progress",
    "watch:debug": "DEBUG=express:* webpack --watch --progress",
    "server": "node dist/server.js",
    "server:debug": "DEBUG=express:* node dist/server.js",
    "lint": "eslint --cache ./src/server.js",
    "lint:fix": "eslint --cache ./src/server.js --fix",
    "lint:all": "eslint ./src/**/*.js --config .eslintrc --parser babel-eslint",
    "lint:test": "eslint ./src/test/**/*.js --config .eslintrc --parser babel-eslint",
    "test": "yarn lint:test && yarn jest",
    "test:fix": "yarn lint:test:fix && yarn jest",
    "test:watch": "yarn jest:watch",
    "test:watch:fix": "yarn lint:test:fix && yarn jest",
    "jest": "jest --config=jest.config.js --forceExit",
    "jest:watch": "jest --config=jest.config.js --watch",
    "clear_jest": "jest --clearCache"
  },
  "nodemonConfig": {
    "ignore": [
      "src/test/*",
      "docs/*"
    ],
    "delay": "3000"
  },
  "dependencies": {
    "async": "^2.6.0",
    "async-redis": "^1.0.0",
    "aws-sdk": "^2.234.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css.escape": "^1.5.1",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.3",
    "express-error-handler": "^1.1.0",
    "express-winston": "^2.5.0",
    "hashids": "^1.1.4",
    "helmet": "^3.12.0",
    "i18n": "^0.8.3",
    "ioredis": "^3.2.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.10",
    "method-override": "^2.3.10",
    "mobile-detect": "^1.4.1",
    "moment": "^2.22.0",
    "mongodb": "^3.1.0-beta4",
    "mongoose": "^5.1.0",
    "mongoose-hidden": "^1.5.3",
    "mongoose-schema-jsonschema": "^1.1.15",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "nodemailer-ses-transport": "^1.5.1",
    "pako": "^1.0.6",
    "redis": "^2.8.0",
    "request-ip": "^2.0.2",
    "response-time": "^2.3.2",
    "serverless-http": "^1.5.5",
    "source-map-support": "^0.5.0",
    "ua-parser-js": "^0.7.18",
    "uuid": "^3.1.0",
    "winston": "^2.4.1",
    "winston-aws-cloudwatch": "^2.0.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "check-dependencies": "^1.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "dotenv-webpack": "^1.5.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "nodemon": "^1.12.1",
    "nodemon-webpack-plugin": "^4.0.3",
    "npm-check": "^5.5.2",
    "npm-run-all": "^4.1.2",
    "portfinder": "^1.0.13",
    "serverless-domain-manager": "^2.4.0",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0"
  }
}
